#ifndef __odin_VERSION

#define __odin_VERSION_MAJOR	1
#define __odin_VERSION_MINOR	2
#define __odin_VERSION_BUILD	312

// make up a version dword
#define __odin_VERSION ((__odin_VERSION_MAJOR << 24) + (__odin_VERSION_MINOR << 16) + (__odin_VERSION_BUILD))

#endif

